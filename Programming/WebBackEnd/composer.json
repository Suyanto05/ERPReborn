{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.1",
        "dragonmantank/cron-expression": "^3.0",
        "elibyy/tcpdf-laravel": "^8.1",
        "fakerphp/faker": "^1.17",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/http": "^8.2",
        "intervention/httpauth": "^3.0",
        "laravel/framework": "^8.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "predis/predis": "^1.1",
        "swaggest/json-schema": "^0.12.31",
        "swiftmailer/swiftmailer": "^6.3",
        "symfony/google-mailer": "^6.0",
        "symfony/mailer": "^6.0",
        "vlucas/phpdotenv": "^5.2"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "vendor/zhtSDK/Device/ALBox/FingerprintAttendance/FP800",
            "vendor/zhtSDK/Device/Goodwin/SwingGateBarrier/ServoSW01",
            "vendor/zhtSDK/Device/Solution/FingerprintAttendance/x601",
            "vendor/zhtSDK/Device/ZKTeco/PersonAttendance/InBio260",
            "vendor/zhtSDK/Library/RegularExpression",
            "vendor/zhtSDK/Software/PDF/TCPDF",
            "vendor/zhtSDK/Software/Excel/Maatwebsite"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
